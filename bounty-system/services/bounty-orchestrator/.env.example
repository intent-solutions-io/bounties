# Bounty Orchestrator Environment Configuration
# Copy to .env and fill in values

# PostgreSQL Database (self-hosted)
# Format: postgresql://user:password@host:port/database
DATABASE_URL=postgresql://bounty_user:password@localhost:5432/intentional_bounty

# Bob's Brain A2A Endpoint
BOBS_BRAIN_A2A_URL=https://a2a-gateway-xxxxx.run.app

# Google Cloud Project
GOOGLE_CLOUD_PROJECT=intentional-bounty

# Optional: LangSmith for observability
# LANGCHAIN_API_KEY=ls__xxxxx
# LANGCHAIN_TRACING_V2=true
# LANGCHAIN_PROJECT=bounty-orchestrator
